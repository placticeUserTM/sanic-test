[mysqld]
character-set-server=utf8
collation-server=utf8_general_ci
explicit-defaults-for-timestamp=1
default-storage-engine=INNODB
default_authentication_plugin=mysql_native_password

[mysqldump]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4

[client]
default-character-set=utf8mb4